}
void mytable(int n)
{
int i;
for(i=1;i<11;i++)
printf("%d\t*\t%d\t=\t%d\n",n,i,n*i);
}
void myprime(int n)
{
int i,count=0;
for(i=2;i<n;i++)
if(n%i==0)
count++;
if(n==i)
printf("NO. IS PRIME\n");
else
printf("NO. IS NOT PRIME\n");
}
void myfact(int n)
{
int i,fact=1;
for(i=0;i<n;i++)
fact=fact*i;
printf("FACTORIAL IS %d\n",fact);
}
